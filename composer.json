{
    "name": "filament/spatie-laravel-media-library-plugin",
    "description": "Filament support for `spatie/laravel-medialibrary`.",
    "license": "MIT",
    "homepage": "https://github.com/laravel-filament/spatie-laravel-medialibrary-plugin",
    "support": {
        "issues": "https://github.com/laravel-filament/spatie-laravel-medialibrary-plugin/issues",
        "source": "https://github.com/laravel-filament/spatie-laravel-medialibrary-plugin"
    },
    "require": {
        "php": "^8.0",
        "filament/forms2": "dev-develop",
        "spatie/laravel-medialibrary": "^9.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.94"
    },
    "autoload": {
        "psr-4": {
            "Filament\\SpatieLaravelMediaLibraryPlugin\\": "src"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpstan"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\SpatieLaravelMediaLibraryPlugin\\SpatieLaravelMediaLibraryPluginServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "local": {
            "type": "path",
            "url": "../forms"
        }
    }
}
